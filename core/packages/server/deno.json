{
  "name": "@wuhu/server",
  "exports": {
    ".": "./main.ts"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4",
    "@hono/hono/cors": "jsr:@hono/hono@^4/cors",
    "@std/assert": "jsr:@std/assert@^1",
    "@std/encoding/base64": "jsr:@std/encoding@^1/base64",
    "@std/path": "jsr:@std/path@^1",
    "@std/yaml": "jsr:@std/yaml@^1",
    "@octokit/rest": "npm:@octokit/rest@^22.0.0",
    "redis": "npm:redis@^4.7.0",
    "drizzle-orm": "npm:drizzle-orm@^0.44.0",
    "drizzle-orm/postgres-js": "npm:drizzle-orm@^0.44.0/postgres-js",
    "drizzle-orm/postgres-js/migrator": "npm:drizzle-orm@^0.44.0/postgres-js/migrator",
    "postgres": "npm:postgres@^3.4.5"
  },
  "tasks": {
    "dev": "deno run -A --watch main.ts",
    "start": "deno run -A main.ts",
    "check": "deno check . && deno lint",
    "test": "deno test -A"
  }
